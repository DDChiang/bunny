<p id="notice"><%= notice %></p>

<p>
  <strong>User name:</strong>
  <%= @member.user_name %>
</p>

<p>
  <strong>First:</strong>
  <%= @member.first %>
</p>

<p>
  <strong>Last:</strong>
  <%= @member.last %>
</p>

<p>
  <strong>Occupation:</strong>
  <%= @member.occupation %>
</p>

<%= link_to 'Edit', edit_member_path(@member) %> |
<%= link_to 'Back', members_path %>

<% @member.recipes.each do |recipe| %>
  <div>
    <%= link_to recipe.name, recipe %>
  </div>
<% end %>
